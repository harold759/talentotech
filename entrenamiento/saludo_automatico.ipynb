        "import datetime\n",
        "\n",
        "def saludo():\n",
        "  hora_actual = datetime.datetime.now().hour\n",
        "\n",
        "  if 0 > hora_actual < 12 :\n",
        "    print('Buenos dias')\n",
        "\n",
        "  elif 13 > hora_actual < 18 :\n",
        "    print('Buenas tardes')\n",
        "\n",
        "  else:\n",
        "    print('Buenas noches')\n",
        "\n",
        "saludo()\n"
      ]
    }
